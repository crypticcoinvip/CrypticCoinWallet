    "spectron": "^3.8.0",
    "style-loader": "^0.22.1",
    "stylefmt": "^6.0.2",
    "stylelint": "^9.4.0",
    "stylelint-config-standard": "^18.2.0",
    "uglifyjs-webpack-plugin": "1.2.7",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.5",
